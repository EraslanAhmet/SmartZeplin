{
	".flSmartface": {
		"flexProps": {
			"flexDirection": "ROW",
			"justifyContent": "CENTER",
			"alignItems": "CENTER"
		},
		"&-label": {
			"width": 60,
			"height": 15,
			"font": {
				"size": 10,
				"bold": true,
				"italic": false,
				"family": "SFProText",
				"style": "Semibold"
			}
		},
		"&-image": {
			"image": "smartface.png",
			"imageFillType": "ASPECTFIT",
			"width": 70,
			"height": 15
		},
		"width": null,
		"height": 20
	}
}